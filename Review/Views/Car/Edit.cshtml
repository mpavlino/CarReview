@model Review.Models.Car.CarViewModel
@{
    ViewData["Title"] = "Car edit";
}
<ol class="breadcrumb">
    <li><a asp-action="Index">Cars list /</a></li>
    <li class="active">&nbsp;Car customization</li>
</ol>
<div class="row">
    <div class="col-lg-12">
        <form asp-action="Edit" method="post" enctype="multipart/form-data">
            @Html.AntiForgeryToken()
            <input type="hidden" asp-for="ID" />
            <input type="hidden" asp-for="ImageData" />
            <partial name="_CreateOrUpdate" />
        </form>
    </div>
</div>